<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
  <changeSet id="create-info-table" author="laurenz-simon">
    <createTable tableName="vs_info">
      <column name="identifierKey" type="VARCHAR(255)">
        <constraints nullable="false" primaryKey="true" primaryKeyName="pk_vs_info"/>
      </column>
      <column name="property_value" type="VARCHAR(255)"/>
    </createTable>
  </changeSet>

</databaseChangeLog>
